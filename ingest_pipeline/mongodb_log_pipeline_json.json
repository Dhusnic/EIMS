{
  "description": "MongoDB JSON log parsing pipeline",
  "processors": [
    {
      "rename": {
        "field": "message",
        "target_field": "event.original",
        "if": "ctx.event?.original == null",
        "ignore_missing": true,
        "ignore_failure": true
      }
    },
    {
      "json": {
        "field": "message",
        "target_field": "mongodb.log"
      }
    }
  ]
}

